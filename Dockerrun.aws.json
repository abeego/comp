{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "abeego/complex-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "api",
			"image": "abeego/complex-api",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "abeego/complex-worker",
			"hostname": "worker",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "abeego/complex-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["client", "api"]
		}
	]
}
